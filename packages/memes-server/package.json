{
  "name": "memes-server",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "dotenv": "^6.2.0",
    "graphql-yoga": "^1.16.7",
    "memes-graph": "0.1.0",
    "ts-node-dev": "^1.0.0-pre.31",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "test": "jest --watch",
    "prettier": "prettier --write",
    "prettier:all": "yarn prettier \"{,!(node_modules)/**/}*\"",
    "lint": "tslint --fix",
    "lint:all": "yarn lint -p .",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0"
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.{ts}": "yarn lint:all",
      "*": "yarn prettier"
    }
  }
}
